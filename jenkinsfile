pipeline {
	Agent any
  environment{
    PATH= /opt/apache-maven-3.6.3/bin:$PATH
  }
	stages{
		stage("git"){
      steps{
			  git credentialsId: 'git_credentials', url: ''
        }
		}
		stage("Static Code Analysis"){
      steps{
			  sh "mvn validate"
        }
		}
    stage("Compile"){
      steps{
			  sh "mvn compile"
        }
		}
    stage("Build"){
      steps{
			  sh "mvn package"
        }
		}
	}

}
